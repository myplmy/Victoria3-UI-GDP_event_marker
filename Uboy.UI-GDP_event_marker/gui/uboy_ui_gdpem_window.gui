    # ==================== 개별 마커 아이콘 ====================
    type event_marker_icon = widget {
        size = { 28 28 }
        parentanchor = left|bottom
        
        block "marker_index" {
            visible = no
        }
        
        block "marker_position" {
            position = { 0 0 }
        }
        
        button = {
            size = { 100% 100% }
            
            block "marker_tooltip" {
                #raw_tooltip = "EVENT_MARKER_TOOLTIP"
            }
            
            icon = {
                size = { 100% 100% }
                parentanchor = center
                
                block "marker_type" {
                    texture = "gfx/interface/icons/ig_icons/armed_forces_30.dds"
                }
                
                # 글로우 효과
                modify_texture = {
                    texture = "gfx/interface/backgrounds/default_bg_shading.dds"
                    spriteType = Corneredstretched
                    blend_mode = overlay
                    alpha = 0.5
                }
            }
            
            # 호버 효과
            state = {
                name = _mouse_hierarchy_enter
                start_sound = {
                    soundeffect = "event:/SFX/UI/Generic/sfx_ui_generic_icon_pointer_over"
                }
            }
        }
    }
    
    # ==================== 범례 컨테이너 ====================
    type event_marker_legend_container = widget {
        layoutpolicy_horizontal = expanding
        
        vbox = {
            layoutpolicy_horizontal = expanding
            spacing = 10
            
            # 타이틀
            textbox = {
                text = "EVENT_MARKER_LEGEND_TITLE"
                layoutpolicy_horizontal = expanding
                autoresize = yes
                align = center|nobaseline
                default_format = "#bold"
            }
            
            # 범례 아이템들
            flowcontainer = {
                direction = horizontal
                parentanchor = hcenter
                spacing = 20
                
                # 전쟁
                event_marker_legend_item = {
                    blockoverride "legend_icon" {
                        texture = "gfx/interface/icons/ig_icons/armed_forces_30.dds"
                    }
                    blockoverride "legend_text" {
                        text = "EVENT_MARKER_LEGEND_WAR"
